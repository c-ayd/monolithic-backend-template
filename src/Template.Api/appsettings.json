{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None",
      "Microsoft.EntityFrameworkCore": "None"
    }
  },
  "AllowedHosts": "*",
  "FlexLog": {
    "BufferLimit": 10000,
    "TimerInSeconds": 5,
    "LogDetails": {
      "Claims": {
        "IncludedTypes": [
          "NameIdentifier",
          "Role"
        ]
      },
      "Headers": {
        "IncludedKeys": [
          "Accept-Language",
          "User-Agent"
        ],
        "Limit": {
          "Length": 512,
          "Strategy": "Slice"
        }
      },
      "RequestBody": {
        "RedactedKeys": [
          "Email",
          "Password",
          "NewEmail"
        ]
      },
      "ResponseBody": {
        "RedactedKeys": [
          "AccessToken"
        ]
      },
      "QueryString": {
        "Limit": {
          "Length": 512
        }
      }
    }
  }
}
